{
   "name": "wonder",
   "version": "0.1.1",
   "private": true,
   "scripts": {
      "predev": "npm run services:up",
      "dev": "next dev --turbo",
      "postdev": "npm run services:stop",
      "prebuild": "next lint --quiet --fix",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "lint:fix": "next lint --quiet --fix",
      "services:up": "docker compose -p drizzle -f infra/compose.yaml up -d",
      "services:stop": "docker compose -p drizzle -f infra/compose.yaml stop",
      "services:down": "docker compose -p drizzle -f infra/compose.yaml down",
      "drizzle:migrate": "drizzle-kit generate",
      "drizzle:push": "drizzle-kit push",
      "drizzle:reset": "dotenv -e .env.development -- drizzle-kit push && tsx ./db/seed.ts"
   },
   "dependencies": {
      "@auth/drizzle-adapter": "^1.8.0",
